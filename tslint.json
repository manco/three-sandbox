{
    "defaultSeverity": "error",
    "jsRules": {},
    "rules": {
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "member-variable-declaration",
            "array-destructuring",
            "object-destructuring",
            "property-declaration"
        ],
        "only-arrow-functions": true,
        "no-boolean-literal-compare": true,
        "no-shadowed-variable": true,
        "no-var-keyword": true
    },
    "rulesDirectory": []
}